{
  "bracketSpacing": true,
  "printWidth": 80,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "all",
  "arrowParens": "always",
  "overrides": [
    {
      "files": ["*.html", "*.scss", "*.css"],
      "options": {
        "tabWidth": 4
      }
    }
  ]
}
